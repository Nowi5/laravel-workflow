{
    "name": "nowis/workflow",
    "description": "Workflow Engine for Laravel",
    "autoload": {
        "psr-4": {
            "Workflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^9.0|^10.0",
        "spatie/laravel-model-states": "^2.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.5",
        "rector/rector": "^0.17.10",
        "symplify/easy-coding-standard": "^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workflow\\Providers\\WorkflowServiceProvider"
            ]
        }
    }
}
